{
  "name": "board",
  "version": "1.0.0",
  "description": "A complete admin board template with a large variety of elements and components, based on blexar CSS framework.",
  "author": "Baianat",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "build:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy": "npm run generate:gh-pages && push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@blexar/framework": "0.0.31",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/style-resources": "^0.1.1",
    "apexcharts": "^2.2.4",
    "driver.js": "^0.8.1",
    "nuxt": "^2.3.1",
    "tiptap": "^0.20.1",
    "tiptap-extensions": "^0.23.0",
    "vee-validate": "^2.1.3",
    "vue-apexcharts": "^1.2.6",
    "vue-flatpickr-component": "^8.0.0",
    "vue-good-table": "^2.16.0",
    "vue-multiselect": "^2.1.3",
    "vue-notification": "^1.3.13",
    "vue2-dropzone": "^3.5.2",
    "vue2-editor": "^2.6.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "prettier": "1.14.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "push-dir": "^0.4.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
